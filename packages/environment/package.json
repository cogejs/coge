{
  "name": "@coge/environment",
  "version": "0.11.2",
  "description": "Handles lookups of generators in a specific environment",
  "license": "MIT",
  "repository": "https://github.com/cogejs/coge",
  "author": "",
  "keywords": [
    "development",
    "dev",
    "build",
    "tool",
    "cli",
    "scaffold",
    "scaffolding",
    "generate",
    "generator",
    "coge",
    "app"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "nb-tsc -b",
    "build:full": "yarn install --ignore-scripts && yarn run clean && yarn run build",
    "precoverage": "yarn test",
    "coverage": "nb-nyc report --reporter=text-summary",
    "prettier:cli": "nb-prettier \"**/*.ts\" \"**/*.js\" \"**/*.md\"",
    "prettier:check": "yarn run prettier:cli --check",
    "prettier:fix": "yarn run prettier:cli --write",
    "format": "yarn run prettier:fix",
    "clean": "del 'dist' 'coverage' '*.tsbuildinfo'",
    "pretest": "yarn run clean && yarn run format && yarn run build",
    "test": "nb-nyc jest",
    "test:ci": "nb-nyc jest"
  },
  "dependencies": {
    "camelcase": "^6.3.0",
    "chalk": "^4",
    "debug": "^4.3.4",
    "diff": "^5.1.0",
    "execa": "^5",
    "fs-extra": "^11.1.1",
    "globby": "^11",
    "inquirer": "^8.2.1",
    "is-scoped": "^2",
    "log-symbols": "^4",
    "semver": "^7.5.4",
    "strip-ansi": "^6",
    "text-table": "^0.2.0",
    "tily": "^2.22.5",
    "untildify": "^4.0.0"
  },
  "devDependencies": {
    "@nutol/build": "^0.2.14",
    "@types/debug": "^4.1.10",
    "@types/diff": "^5.0.7",
    "@types/fs-extra": "^11.0.3",
    "@types/inquirer": "^9.0.6",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.9",
    "@types/sinon": "^10.0.20",
    "@types/text-table": "^0.2.4",
    "cross-env": "^7.0.3",
    "del-cli": "^5.1.0",
    "dir-compare": "^4.2.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "sinon": "^17.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  }
}
