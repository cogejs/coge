{
  "name": "@coge/environment",
  "version": "0.9.1",
  "description": "Handles lookups of generators in a specific environment",
  "license": "MIT",
  "repository": "https://github.com/cogejs/@coge/environment",
  "author": "",
  "keywords": [
    "development",
    "dev",
    "build",
    "tool",
    "cli",
    "scaffold",
    "scaffolding",
    "generate",
    "generator",
    "coge",
    "app"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prerelease": "cross-env CI=1 npm run test",
    "release": "np --no-2fa",
    "build": "lb-tsc -b",
    "build:full": "npm ci --ignore-scripts && npm run clean && npm run build",
    "precoverage": "npm test",
    "coverage": "lb-nyc report --reporter=text-summary",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\" \"**/*.md\"",
    "prettier:check": "npm run prettier:cli -- --check",
    "prettier:fix": "npm run prettier:cli -- --write",
    "format": "npm run prettier:fix",
    "clean": "lb-clean dist coverage .nyc_output *.tsbuildinfo",
    "pretest": "npm run clean && npm run format && npm run build",
    "test": "lb-nyc jest",
    "test:ci": "lb-nyc jest"
  },
  "dependencies": {
    "camelcase": "^6.2.1",
    "chalk": "^4",
    "debug": "^4.3.3",
    "diff": "^5.0.0",
    "execa": "^5",
    "fs-extra": "^10.0.0",
    "globby": "^11",
    "inquirer": "^8.2.0",
    "is-scoped": "^2",
    "log-symbols": "^4",
    "semver": "^7.3.5",
    "strip-ansi": "^6",
    "text-table": "^0.2.0",
    "tily": "^2.9.5",
    "untildify": "^4.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^8.0.0",
    "@types/debug": "^4.1.7",
    "@types/diff": "^5.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.1.3",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.4",
    "@types/sinon": "^10.0.6",
    "@types/text-table": "^0.2.2",
    "cross-env": "^7.0.3",
    "dir-compare": "^4.0.0",
    "jest": "^27.4.5",
    "np": "^7.6.0",
    "rimraf": "^3.0.2",
    "sinon": "^12.0.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ]
}
