{
  "name": "coge",
  "version": "0.11.11",
  "description": "An efficient code generator.",
  "license": "MIT",
  "repository": "https://github.com/cogejs/coge",
  "author": "ty <towyuan@outlook.com>",
  "keywords": [
    "coge",
    "cli",
    "development",
    "dev",
    "build",
    "tool",
    "scaffold",
    "stack",
    "app",
    "generate",
    "generator",
    "generation",
    "template",
    "boilerplate",
    "redux",
    "react"
  ],
  "files": [
    "bin",
    "dist",
    "generators"
  ],
  "bin": {
    "co": "bin/coge",
    "coge": "bin/coge"
  },
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "release": "np --no-2fa",
    "standalone": "rimraf standalone; yarn run build && pkg package.json --out-path standalone && yarn run standalone:postprocess",
    "standalone:postprocess": "node scripts/standalone.js",
    "standalone:brew-sha": "shasum -a 256 standalone/*.macos.*.tar.gz",
    "clean": "nb-clean dist *.tsbuildinfo",
    "build": "nb-tsc -b",
    "build:full": "yarn install --ignore-scripts && yarn run clean && yarn run build",
    "test": "yarn run build && yarn run test:code && yarn run test:require",
    "test:code": "jest",
    "test:win32": "jest --runInBand metaverse",
    "test:metaverse": "jest metaverse",
    "test:require": "mkdirp temp; cd temp; node ../dist/cli.timed.js init; cd ..; rm -rf temp"
  },
  "dependencies": {
    "@coge/environment": "^0.10.4",
    "@coge/generator": "^0.6.8",
    "@iarna/toml": "^2.2.5",
    "caporal": "^1.4.0",
    "chalk": "^4",
    "change-case": "^4.1.2",
    "cross-spawn": "^7.0.3",
    "debug": "^4.3.3",
    "diff": "^5.0.0",
    "ejs": "^3.1.6",
    "execa": "^5",
    "front-matter": "^4.0.2",
    "fs-extra": "^10.0.1",
    "globby": "^11",
    "ignore-walk": "^4.0.1",
    "inflection": "^1.13.2",
    "inquirer": "^8.2.0",
    "is-scoped": "^2",
    "json5": "^2.2.0",
    "repeat-string": "^1.6.1",
    "tily": "^2.14.3",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@nutol/build": "^0.2.4",
    "@types/ejs": "^3.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/ignore-walk": "^4.0.0",
    "@types/inflection": "^1.13.0",
    "@types/inquirer": "^8.2.0",
    "@types/jest": "^27.4.1",
    "@types/json5": "^0.0.30",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^17.0.21",
    "@types/repeat-string": "^1.6.3",
    "dir-compare": "^4.0.0",
    "jest": "^27.5.1",
    "lodash": "^4.17.21",
    "micromatch": "^4.0.4",
    "np": "^7.6.0",
    "pkg": "^5.5.2",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "time-require": "^0.1.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": "src/**/*",
    "outPath": "standalone",
    "targets": [
      "node12-macos",
      "node12-linux",
      "node12-win"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
